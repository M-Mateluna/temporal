function showMsgLocal(){
  MSGBOX.Abrir({
    titulo: 'Informaci√≥n',
    texto: 'Hola desde el lightbox hijo',
    botonera: 1,
    fnretorno: 'cerrarMsgLocal'   // <- agregado
  });
}

function cerrarMsgLocal(resp){
  // Cierra el MessageBox al presionar Aceptar (resp === 'A')
  MSGBOX.ventana.HideInfo();
}
